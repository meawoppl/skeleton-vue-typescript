{
    "name": "skeleton-vue-typescript",
    "description": "A web frontend skeleton project with Vue.js, TypeScript, Bootstrap, Less, Webpack, and Jest.",
    "version": "0.2.0",
    "license": "Apache-2.0",
    "author": "Justus Bogner <jreb1990@gmail.com>",
    "scripts": {
        "build": "webpack --config webpack.prod.config.js --progress",
        "start": "webpack-dev-server --open --history-api-fallback --hot --config webpack.dev.config.js",
        "start-prod": "webpack-dev-server --open --history-api-fallback --config webpack.prod.config.js",
        "lint": "tslint -c tslint.json src/**/*.ts",
        "test": "jest",
        "sonar": "sonar-scanner"
    },
    "jest": {
        "testURL": "http://localhost",
        "moduleFileExtensions": [
            "ts",
            "js",
            "json",
            "vue"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,vue}",
            "!**/node_modules/**"
        ],
        "coverageReporters": [
            "html",
            "text-summary"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.21",
        "@fortawesome/free-solid-svg-icons": "^5.10.1",
        "@fortawesome/vue-fontawesome": "^0.1.6",
        "axios": "^0.19.0",
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.0-rc.28",
        "bootswatch": "^4.3.1",
        "core-js": "^3.2.1",
        "moment": "^2.24.0",
        "vue": "2.6.10",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.2.1",
        "vue-router": "^3.1.2"
    },
    "devDependencies": {
        "@types/jest": "^24.0.17",
        "@vue/test-utils": "^1.0.0-beta.29",
        "babel-core": "^6.26.3",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "file-loader": "^4.2.0",
        "git-revision-webpack-plugin": "^3.0.3",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "less": "^3.10.1",
        "less-loader": "^5.0.0",
        "sonarqube-scanner": "^2.5.0",
        "string-replace-loader": "^2.2.0",
        "ts-jest": "^24.0.2",
        "ts-loader": "^6.0.4",
        "tslint": "^5.18.0",
        "tslint-loader": "^3.5.4",
        "tslint-webpack-plugin": "^2.1.0",
        "typescript": "^3.5.3",
        "vue-jest": "^3.0.4",
        "vue-loader": "^15.7.1",
        "vue-template-compiler": "2.6.10",
        "webpack": "^4.39.2",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0",
        "webpack-merge": "^4.2.1"
    }
}
